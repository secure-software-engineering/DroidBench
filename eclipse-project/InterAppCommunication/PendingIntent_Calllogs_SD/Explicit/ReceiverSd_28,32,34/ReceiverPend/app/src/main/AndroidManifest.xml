<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mnit.receiverpend">
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <receiver android:name="com.example.mnit.receiverpend.ReceiverPend">
            <intent-filter><action android:name="com.example.mnit.receiverpend.ReceiverPend"/>
            <category android:name="android.intent.category.DEFAULT"/></intent-filter></receiver>
    </application>

</manifest>
